{
    "name": "robrogers3/laraldap-auth",
    "type": "library",
    "description": "ldap authentication for laravel",
    "keywords": [
        "robrogers3",
        "laraldap-auth",
        "laravel",
        "ldap",
        "authentication",
        ""
    ],
    "homepage": "https://github.com/robrogers3/laraldap-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Rogers",
            "email": "robrogers@me.com",
            "homepage": "https://github.com/robrogers3",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/support": "5.3.* || 5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "robrogers3\\laradauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "robrogers3\\laradauth\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
